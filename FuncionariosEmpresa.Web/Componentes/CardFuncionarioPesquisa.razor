<MudCard>
    <MudCardMedia Image="images/Perfil.png" Style="width: 80px; height: 80px;"></MudCardMedia>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Funcionario!.Nome</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Funcionario!.Cpf</MudText>
    </MudCardContent>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Funcionario!.Email</MudText>
    </MudCardContent>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Funcionario!.NumeroCelular</MudText>
    </MudCardContent>
</MudCard>


@code {
    [Parameter]
    public ExibeFuncionarioDto? Funcionario { get; set; }
}

